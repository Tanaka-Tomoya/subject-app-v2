{
  "overwrite": true,
  "schema": "http://localhost:4000/graphql",
  "documents": null,
  "generates": {
    "src/types/generated.ts": {
      "plugins": [
        "typescript",
        "typescript-resolvers",
        "typescript-document-nodes"
      ]
    },
    "./graphql.schema.json": { "plugins": ["introspection"] }
  },
  "config": {
    "addUnderscoreToArgsType": true
  }
}
